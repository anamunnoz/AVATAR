@using Microsoft.AspNetCore.Components.Web
@namespace Cards_Game.Visual.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href = "css/Inicio.css" rel = "stylesheet" />
    <link href = "css/Interprete.css" rel = "stylesheet" />
    <link href = "css/Campo.css" rel = "stylesheet" />
    <link href="Cards-Game.Visual.styles.css" rel="stylesheet" />
    <link href="https://fonts.cdnfonts.com/css/friendly-felt-tips" rel="stylesheet">
    <link href="https://fonts.cdnfonts.com/css/lacquer" rel="stylesheet">
    <link href="favicon (2).ico" rel="icon" type="image/x-icon">
                


    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body>
    @RenderBody()
    
    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
        

    <script src="_framework/blazor.server.js"></script>
</body>
</html>
